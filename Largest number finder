#include <iostream>
using namespace std;

int main() {
    int x;
    cout<<"enter number";
    cin>>x;
    int y;
    cout<<"enter number";
    cin>>y;
    int z;
    cout<<"enter number";
    cin>>z;
    if(x > y && x> z)
    cout<<x<<"is biggest";
    else if(y > x &&y > z)
    cout<<y<<"is biggest";
    else if (z > y && z> x)
    cout<<z<<"is biggest";
    else 
    cout<<"same numbers";
    return 0;
}

